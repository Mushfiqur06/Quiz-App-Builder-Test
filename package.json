{
  "name": "quize-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@popperjs/core": "^2.4.2",
    "@reach/combobox": "^0.8.0",
    "@reach/component-component": "^0.11.1",
    "@reach/menu-button": "^0.8.0",
    "@reach/tabs": "^0.8.0",
    "@reach/tooltip": "^0.8.0",
    "@reach/window-size": "^0.11.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/js-cookie": "^2.2.7",
    "@types/node": "^16.6.1",
    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "@types/redux-logger": "^3.0.9",
    "autoprefixer": "^9",
    "axios": "^0.21.1",
    "formik": "^2.2.9",
    "js-cookie": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "node-sass": "4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-popper": "^2.2.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-spinners": "^0.10.6",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "twin.macro": "^1.6.3",
    "twind": "^0.14.6",
    "typescript": "^4.3.5",
    "react-spring": "8.0.27",
    "classnames": "2.2.6",
    "web-vitals": "^1.0.1"
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "scripts": {
    "build:tailwind": "postcss src/style/tailwind.css -o src/style/tailwind.generated.css",
    "watch:tailwind": "postcss -w src/style/tailwind.css -o src/style/tailwind.generated.css",
    "start": "run-p watch:tailwind start:react",
    "start:react": "react-scripts start",
    "prebuild": "npm run build:tailwind",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint . --ext ts --ext tsx --ext js",
    "format": "npx prettier --write **/*.{js,ts,tsx}",
    "type-check": "tsc --pretty --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/styled-components": "5.1.0",
    "@types/classnames": "2.2.6"
  }
}
